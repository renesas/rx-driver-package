    <toolchain>
        <brand>IAR</brand>
        <product>ICCRX</product>
        <version>4.20.01</version>
    </toolchain>

    <!-- only one target entry allowed; multiple sub tags permitted -->
    <target>
        <family>RX</family>
        <series>RX200</series>
        <series>RX600</series>
        <series>RX700</series>
        <group>RX231</group>
        <group>RX23W</group>
